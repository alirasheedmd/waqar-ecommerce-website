{
  "name": "proshop",
  "version": "1.0.0",
  "description": "MERN Shopping Cart App",
  "main": "server.js",
  "type": "module",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.7",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "react-bootstrap": "^1.3.0",
    "slugify": "^1.6.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
  },
  "scripts": {
    "start": "nodemon backend/server.js ",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run start\" \"npm run client\"",
